<h1>Jogos</h1>

<a href="/jogos/novo">Cadastrar novo jogo</a>

<table border="1">
  <tr>
    <th>Título</th>
    <th>Descrição</th>
    <th>Preço Base</th>
  </tr>
  {{#each jogos}}
    <tr>
      <td>{{this.titulo}}</td>
      <td>{{this.descricao}}</td>
      <td>{{this.precobase}}</td>
      <td>
        <a href="/jogos/{{this.id}}/conquistas">Ver Conquistas</a>
      </td>
    </tr>
  {{/each}}
</table>